{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "SignUpInput",
    "$protected": false,
    "type":"object",
    "properties": {
        "username":{
            "type":"string"
        },
        "password": {
            "type":"string",
            "minLength": 8
        },
        "email":{
            "type":"string",
            "pattern": "^[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,}$"
        },
        "first_name":{
            "type":"string",
            "maxLength": 50
        },
        "last_name":{
            "type":"string",
            "maxLength": 50
        }
    },
    "required": [
        "username",
        "password",
        "email"
    ]
}